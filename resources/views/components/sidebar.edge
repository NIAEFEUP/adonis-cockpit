@let(items = admin.getMenu())
<div
  class="fixed bottom-0 top-16 z-20 hidden w-72 overflow-auto bg-white md:flex border-neutral-300 dark:border-neutral-700 dark:bg-neutral-950 border-r"
>
  <div class="flex flex-col px-8 py-8">
    @each(item in admin.getMenu())
      @let([component, props] = await item.render())
      @!component(component, props)
    @end
  </div>
</div>
